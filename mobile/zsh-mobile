bindkey -s 'qq' '`'
bindkey -s 'qQ' '~'
bindkey -s $'q\'' '~'

bindkey -s 'qo' '['
bindkey -s 'qp' ']'

bindkey -s 'qO' '{'
bindkey -s 'q9' '{'
bindkey -s 'qP' '}'
bindkey -s 'q0' '}'

bindkey -s 'q\e[A' '|'
bindkey -s 'q\e[B' '\\'
bindkey -s 'q\e[C' '>'
bindkey -s 'q\e[D' '<'

alias vim='vim -S ~/royal-neko/mobile/vimrc-mobile'
