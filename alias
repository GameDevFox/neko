# this allows us to use "sudo" with aliases
alias sudo='sudo '

# ls
alias l='ls -CF'
alias la='ls -A'
alias ll='ls -al'
alias ls='ls --color=auto'

# git
alias gs='git status'
alias gd='git diff'
alias gb='git branch'
alias gbb='git branch -avv'
alias gc='git checkout'
alias gp='git pull'

# mobile
alias mvim='vim -S ~/royal-neko/mobile/vimrc-mobile'
