projects-list | xargs -IX bash -c '
cd X
git branch --remote | grep "origin/master" >/dev/null 2>&1
exitCode="$?"

if [ "$exitCode" = "0" ]; then
  commits=$(git-outdated)
  files=$(git status --short | wc -l)
  echo "$commits / $files commits ahead in X"
else
  echo "----- commits ahead in X"
fi
'
